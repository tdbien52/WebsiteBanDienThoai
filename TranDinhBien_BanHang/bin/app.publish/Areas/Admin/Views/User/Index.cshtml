@model List<TranDinhBien_BanHang.Identity.AppUser>
@{
    ViewBag.Title = "Index";
}

<h2>Danh mục khách hàng</h2>
<table class="table table-bordered">
    <tr>
        <th>Tên người dùng</th>
        <th>E-Mail</th>
        <th>Số điện thoại</th>
        <th>Thành phố</th>
        <th>Ngày sinh</th>
        <th>Thao Tác</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.UserName</td>
            <td>@item.Email</td>
            <td>@item.PhoneNumber</td>
            <td>@item.City</td>
            <td>@item.Birthday</td>
            <td>
                <a href="/admin/user/edit/@item.Id">Sửa</a> |
                <a href="/admin/user/delete/@item.Id">Xóa</a>
            </td>

        </tr>

    }
</table>

